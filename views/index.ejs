        .card.storycard {
            width: 100% !important;
            object-fit: cover !important;
            transition: all 0.4s ease-in-out 0s !important;
            z-index: 0 !important;
        }

        .card.storycard::before {
            content: "";
            position: absolute;
            width: 100%;
            height: 100%;
            background: rgba(0, 0, 0, 0.44);
            inset: 0px;
            transition: all 0.4s ease-in-out 0s;
            transform: scale(0);
            z-index: 1;
        }

        .card.storycard .leaf {
            position: absolute;
            text-align: center;
            width: 100%;
            top: 42%;
            transform: translateY(15px) scale(0);
            transition: all 0.4s ease-in-out 0s;
            opacity: 0.6;
            z-index: 2;
        }


        .card.storycard .leaf::before {
            margin-left: -161px;
            content: "";
            position: absolute;
            width: 150px;
            height: 184px;
            background: url("/img/leftleafs.png") 0% 0% / 100% no-repeat;
            top: -71px;
            transition: all 0.4s ease-in-out 0s;

        }

        .card.storycard .leaf::after {
            margin-right: -6px;
            transform: scaleX(-1);
            content: "";
            position: absolute;
            width: 150px;
            height: 184px;
            background: url("/img/leftleafs.png") 0% 0% / 100% no-repeat;
            top: -71px;
            transition: all 0.4s ease-in-out 0s;

        }

        .card.storycard img {
            width: 100%;
            transition: all 0.4s ease-in-out 0s;
            z-index: 0;
            height: 400px;
        }

        .card.storycard .bx {
            position: absolute;
            top: 42%;
            transition: all 0.4s ease-in-out 0s;
            transform: translateY(30px);
            opacity: 0;
            z-index: 1;
            width: 100%;
            text-align: center;
            color: #fff !important;

        }

        .card.storycard:hover img {
            transform: scale(1.08);
        }

        .card.storycard:hover .bx {
            opacity: 1;
            transform: translateY(0);
        }

        .card.storycard:hover .leaf {
            transform: translateY(0px) scale(1);
            opacity: 0.7;
        }

        .card.storycard:hover::before {
            transform: scale(1);
        }

