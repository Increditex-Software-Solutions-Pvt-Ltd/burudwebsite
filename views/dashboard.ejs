    <script src="lib/waypoints/waypoints.min.js"></script>
    <script src="lib/counterup/counterup.min.js"></script>
    <script src="lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="lib/tempusdominus/js/moment.min.js"></script>
    <script src="lib/tempusdominus/js/moment-timezone.min.js"></script>
    <script src="lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>



    <script src="js/main.js"></script>
    <script src="js/profile.js"></script>
    <script src="/js/preventinspect.js"></script>



    <script>
        const sentBody = document.getElementById("sentBody")
        const receivedBody = document.getElementById("receivedBody")
        const connectionCount = document.getElementById("connectionCount")
        const receivedCount = document.getElementById("receivedCount")
        const sentCount = document.getElementById("sentCount")


        async function getList() {
            let sentArr, receivedArr
            await $.ajax({
                type: 'GET',
                url: '/getListofRequests',
                success: function (response) {
                    // Handle success response

                    sentArr = [...response.sentArr]
                    receivedArr = [...response.receivedArr]

                    console.log(sentArr, receivedArr, "Data from backend checking....")

                },
                error: function (xhr, status, error) {
                    // Handle error
                    console.error(error);
                }
            });

            if (sentArr.length) {
                sentCount.innerText = sentArr.length
                sentBody.innerHTML = ""
                for (user of sentArr) {
                    let createrow = document.createElement("tr")

                    let nametd = document.createElement("td")
                    nametd.innerText = `${user.firstname} ${user.middlename} ${user.lastname}`

                    let statustd = document.createElement("td")
                    statustd.innerText = `${user.district}`

                    let actiontd = document.createElement("td")
                    let viewbtn = document.createElement("button")
                    viewbtn.style.color = "white"
                    viewbtn.style.backgroundColor = "#343a40"
                    viewbtn.style.border = "none"
                    viewbtn.style.padding = "3px 7px"
                    viewbtn.innerText = "View Profile"
                    viewbtn.style.fontSize = "13px"

                    let profileId = undefined
                    await $.ajax({
                        type: 'GET',
                        url: '/getProfileid',
                        data: { id: user.id },
                        success: function (response) {
                            // Handle success response
                            profileId = response.profileId
                            console.log(response.profileId)

                        },
                        error: function (xhr, status, error) {
                            // Handle error
                            console.error(error);
                        }
                    });

                    viewbtn.addEventListener('click', async () => {
                        window.location.href = `/detailprofile/${profileId}`
                    })
                    actiontd.appendChild(viewbtn)
                    createrow.append(nametd, statustd, actiontd)
                    sentBody.appendChild(createrow)
                }
            } else {
                sentCount.innerText="0"
                sentBody.innerHTML = ""
                let noData = document.createElement("tr")
                noData.classList = "text-center fs-5"
                noData.style.color = "#66451c";
                let noDatatd = document.createElement("td")
                noDatatd.colSpan = "3"
                noDatatd.innerText = "No Requests Sent"
                noData.appendChild(noDatatd)
                sentBody.append(noData)
            }

            if (receivedArr.length) {
                console.log(receivedArr,"Received Array of User...");
                receivedCount.innerText=receivedArr.length
                receivedBody.innerHTML = ""
                for (user of receivedArr) {
                    let createrow = document.createElement("tr")

                    let nametd = document.createElement("td")
                    nametd.innerText = `${user.firstname} ${user.middlename} ${user.lastname}`

                    let statustd = document.createElement("td")
                    statustd.innerText = `${user.district}`

                    let actiontd = document.createElement("td")
                    let viewbtn = document.createElement("button")
                    viewbtn.style.color = "white"
                    viewbtn.style.backgroundColor = "#343a40"
                    viewbtn.style.border = "none"
                    viewbtn.style.padding = "3px 7px"
                    viewbtn.innerText = "View Profile"
                    viewbtn.style.fontSize = "13px"
                    let profileId = undefined
                    await $.ajax({
                        type: 'GET',
                        url: '/getProfileid',
                        data: { id: user.id },
                        success: function (response) {
                            // Handle success response
                            profileId = response.profileId
                            console.log(response.profileId)

                        },
                        error: function (xhr, status, error) {
                            // Handle error
                            console.error(error);
                        }
                    });

                    viewbtn.addEventListener('click', async () => {
                        window.location.href = `/detailprofile/${profileId}`
                    })
                    actiontd.appendChild(viewbtn)

                    createrow.append(nametd, statustd, actiontd)
                    receivedBody.appendChild(createrow)
                }
            } else {
                receivedCount.innerText="0"
                receivedBody.innerHTML = ""
                let noData = document.createElement("tr")
                noData.classList = "text-center fs-5"
                noData.style.color = "#66451c";
                let noDatatd = document.createElement("td")
                noDatatd.colSpan = "3"
                noDatatd.innerText = "No Requests Received"
                noData.appendChild(noDatatd)
                receivedBody.append(noData)
            }

        }
        getList()


        async function getListC() {
            let connectionsArr
            await $.ajax({
                type: 'GET',
                url: '/getListofConections',
                success: function (response) {
                    // Handle success response

                    connectionsArr = [...response.connectionsArr]

                },
                error: function (xhr, status, error) {
                    // Handle error
                    console.error(error);
                }
            });
            const connectionBody = document.getElementById("connectionBody")
            if (connectionsArr.length) {
                connectionCount.innerText=connectionsArr.length
                connectionBody.innerHTML = ""
                for (user of connectionsArr) {
                    if (user !== null) {
                        let createrow = document.createElement("tr")

                        let nametd = document.createElement("td")
                        nametd.innerText = `${user.firstname} ${user.middlename} ${user.lastname}`

                        let emailtd = document.createElement("td")
                        emailtd.innerText = `${user.email}`

                        let phonetd = document.createElement("td")
                        phonetd.innerText = `${user.phone}`

                        let statustd = document.createElement("td")
                        statustd.innerText = `${user.district}`

                        let actiontd = document.createElement("td")
                        let viewbtn = document.createElement("button")
                        viewbtn.style.color = "white"
                        viewbtn.style.backgroundColor = "#343a40"
                        viewbtn.style.border = "none"
                        viewbtn.style.padding = "3px 7px"
                        viewbtn.innerText = "View Profile"
                        viewbtn.style.fontSize = "13px"

                        let profileId = undefined
                        await $.ajax({
                            type: 'GET',
                            url: '/getProfileid',
                            data: { id: user.id },
                            success: function (response) {
                                // Handle success response
                                profileId = response.profileId

                            },
                            error: function (xhr, status, error) {
                                // Handle error
                                console.error(error);
                            }
                        });

                        viewbtn.addEventListener('click', async () => {
                            window.location.href = `/detailprofile/${profileId}`
                        })
                        actiontd.appendChild(viewbtn)
                        createrow.append(nametd, emailtd, phonetd, statustd, actiontd)
                        connectionBody.appendChild(createrow)
                    }

                }
            } else {
                connectionCount.innerText="0"
                connectionBody.innerHTML = ""
                let noData = document.createElement("tr")
                noData.classList = "text-center fs-5"
                noData.style.color = "#66451c";
                let noDatatd = document.createElement("td")
                noDatatd.colSpan = "3"
                noDatatd.innerText = "No Connections"
                noData.appendChild(noDatatd)
                connectionBody.append(noData)
            }

        }

        getListC()
    </script>

</body>

